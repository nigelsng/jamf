<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Description</key>
	<string>Downloads the current release version of Cisco Webex Meetings and extracts the installer package.</string>
	<key>Identifier</key>
	<string>com.homedepot.pkg.ciscowebex</string>
	<key>Input</key>
	<dict>
		<key>VENDOR</key>
		<string>Cisco</string>
		<key>NAME</key>
		<string>Cisco Webex Meetings</string>
	</dict>
	<key>MinimumVersion</key>
	<string>1.0</string>
	<key>ParentRecipe</key>
	<string>com.homedepot.download.ciscowebex</string>
	<key>Process</key>
	<array>
		<dict>
			<key>Comment</key>
			<string>Copy the package out of the disk image.</string>
			<key>Arguments</key>
			<dict>
				<key>source_pkg</key>
				<string>%pathname%/%NAME%.pkg</string>
				<key>pkg_path</key>
				<string>%RECIPE_CACHE_DIR%/%NAME%-%version%.pkg</string>
			</dict>
			<key>Processor</key>
			<string>PkgCopier</string>
		</dict>
	</array>
</dict>
</plist>
